{
  "name": "physical-web-scan-app",
  "version": "0.3.2",
  "description": "Electron app for Mac OSX to scan for Physical Web (Eddystone) beacons",
  "main": "main.js",
  "scripts": {
    "package": "node ./node_modules/electron-packager/cli.js ./ PhysicalWebScan --platform=darwin --arch=x64 --electron-version=1.8.4 --ignore=node_modules/electron-rebuild --asar --icon=icon.icns",
    "rebuild": "node ./node_modules/.bin/electron-rebuild",
    "electron": "node ./node_modules/electron/cli.js ."
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/dermike/electron-physical-web-scan.git"
  },
  "keywords": [
    "electron",
    "physical web"
  ],
  "author": "Mikael Jergefelt",
  "license": "ISC",
  "devDependencies": {
    "electron": "1.8.4",
    "electron-packager": "11.2.0",
    "electron-rebuild": "1.7.3"
  },
  "dependencies": {
    "noble": "1.9.1"
  }
}
